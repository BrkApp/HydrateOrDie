{
  "expo": {
    "name": "HydrateOrDie",
    "slug": "hydrate-or-die",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#1A1B2E",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1B2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hydrateordie.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A1B2E"
      },
      "package": "com.hydrateordie.app",
      "permissions": [
        "CAMERA",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "VIBRATE"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#EF476F"
        }
      ]
    ]
  }
}
